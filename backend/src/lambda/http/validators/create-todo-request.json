{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Insert new Todo request validator",
  "type": "object",
  "properties": {
    "name":{
      "type": "string",
      "minLength": 3
    },
    "dueDate": {
      "type": "string"
    },
    "createdAt": {
      "type": "string"
    },
    "done": {
      "type": "boolean"
    },
    "attachmentUrl": {
      "type": "string"
    }
  },
  "required": [
    "name",
    "dueDate"
  ],
  "additionalProperties": false
}